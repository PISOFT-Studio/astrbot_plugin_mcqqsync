{
  "ws_host": {
    "description": "Websocket 监听地址",
    "type": "string",
    "default": "127.0.0.1",
    "hint": "例如 127.0.0.1 或者 你服务器公网ip"
  },
  "ws_port": {
    "description": "Websocket 监听端口号",
    "type": "int",
    "default": 52778
  },
  "group_ids": {
    "description": "群id",
    "type": "list",
    "hint": "/sid获取的适配器名",
    "items": {
      "type": "string"
    },
    "default": []
  },
  "provider_id": {
    "description": "模型提供商ID",
    "type": "string",
    "default": "openai",
    "hint": "没啥可说的"
  },
  "expected_token": {
    "description": "插件端的token",
    "type": "string",
    "default": "token",
    "hint": "mc服务器装上插件会在控制台输出一个token，在控制台执行/mcqqsync token get也可以获取"
  }
}